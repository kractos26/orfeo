--dia_sig_ant(): Recibe como parametros la fecha de inicio y la palabra Proximo ó Anterior, y devolverá el proximo o anterior dia habil.
-------------------------------------------
create or replace
FUNCTION INGBDORFEO.dia_sig_ant(fecha_inicio in date, proxant in varchar2)
  RETURN date
AS
    diaSemana varchar2(10);
    nuevoDia date;
BEGIN
        diaSemana :=  to_char(fecha_inicio,'DY');
        IF (proxant='Anterior') then
            IF (diaSemana = 'LUN' OR diaSemana = 'MON') then
                 nuevoDia := fecha_inicio - 3 ;
            ELSE
                 nuevoDia := fecha_inicio -1 ;
            end if;

        ELSE IF (proxant='Proximo') then
            IF (diaSemana = 'VIE' OR diaSemana = 'FRI') then
                 nuevoDia := fecha_inicio +3 ;
            ELSE
                nuevoDia := fecha_inicio + 1 ;
            end if;
        ELSE
             nuevoDia := fecha_inicio;
        end if;
        end if;
        RETURN nuevoDia;
END dia_sig_ant;

----------------------------------------------------
--sumadiashabiles(): Esta funcion recibe como parametros la fecha de inicio y el numero de dias habiles
--y retornara la fecha correspondiente teniendo en cuanta los dias festivos entre semana.
----------------------------------------------------
create or replace
FUNCTION INGBDORFEO.sumadiashabiles(fecinicio in date, numdias in integer)
  RETURN date AS
    contador integer;
    diaSemana varchar2(10);
    fechafin date;
    diasNH number;
BEGIN

    IF (NumDias>0) THEN
        contador:=1;
        fechaFin:=FecInicio;
        WHILE (contador <= NumDias) LOOP
            fechaFin := INGBDORFEO.dia_sig_ant(fechaFin,'Proximo' );
            contador := contador+1;
        END LOOP;
    ELSE
        contador:=ABS(NumDias);
        fechaFin:=to_char(FecInicio,'YYYY/mm/dd');
        WHILE (contador>0 )LOOP
            fechaFin := INGBDORFEO.dia_sig_ant(fechaFin,'Anterior');
            contador:=contador-1;
        END LOOP;
    END IF;
    select count(*) into diasNH from INGBDORFEO.sgd_noh_nohabiles where noh_fecha between fecinicio and fechaFin;
    fechaFin := fechaFin+ diasNH;
    diaSemana :=  to_char(fechaFin,'DY');
    IF (diaSemana = 'SAT' or diaSemana = 'SUN' or diaSemana = 'SÁB' or diaSemana = 'DOM') then
            fechaFin := fechaFin + 2;
    end if;
    RETURN fechafin;
END sumadiashabiles;

----------------------------------------------------------
--diashabiles():Funcion que recibe como parametros fecha inicial y fecha final
--y retorna dias habiles que han transcurrido entre dichas fechas teniendo en cuenta los festivos.
----------------------------------------------------------
create or replace
FUNCTION INGBDORFEO.diashabiles(fecinicio in date, fecfin in date)
  RETURN number
  AS
    fechaAnterior date;
    dias number;
    diasNH number;
BEGIN
    dias:=0;
    fechaAnterior:= FecInicio;
    select count(*) into diasNH from sgd_noh_nohabiles where noh_fecha between fecInicio and fecFin;
    if(fechaAnterior<=fecFin) then
	    fechaAnterior := INGBDORFEO.dia_sig_ant(fechaAnterior,'Proximo' );
	    WHILE (fechaAnterior<= fecFin ) LOOP
		    fechaAnterior := INGBDORFEO.dia_sig_ant(fechaAnterior,'Proximo' );
		    dias := dias-1;
	    END LOOP;
	    dias:=dias+diasNH;
    else
	fechaAnterior:=FecInicio;
	WHILE (fechaAnterior > fecFin ) LOOP
		    fechaAnterior := INGBDORFEO.dia_sig_ant(fechaAnterior,'Anterior' );
		    dias := dias+1;
	END LOOP;
	dias:=dias-diasNH;
    end if;
    RETURN dias;
END diashabiles;